{
  "expo": {
    "name": "LearningApp",
    "slug": "LearningApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/myapp.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/myapp.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "ca.mark.learningapp",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to take photos.",
        "NSPhotoLibraryAddUsageDescription": "This app needs access to the photo library to save photos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/myapp.png",
        "backgroundColor": "#ffffff"
      },
      "package": "ca.mark.learningapp",
      "versionCode": 1000,
      "permissions": [
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE",
        "RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "8fef2744-7c77-4cd3-b741-15171cec9ffd"
      }
    }
  }
}
